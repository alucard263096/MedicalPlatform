<?xml version="1.0" encoding="utf-8" ?>
<root>
  <name>加盟申请</name>
  <tablename>tb_requisition</tablename>
  <nolist>0</nolist>
  <searchcondition>r_main.status&lt;&gt;'D' </searchcondition>
  <fields>
    <field>
      <name>申请人姓名</name>
      <key>name</key>
      <type>text</type>
      <displayinlist>1</displayinlist>
      <gotodetail>1</gotodetail>
      <search>1</search>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>职位</name>
      <key>position</key>
      <type>text</type>
      <displayinlist>1</displayinlist>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>申请人邮箱</name>
      <key>email</key>
      <type>text</type>
      <displayinlist>1</displayinlist>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>申请人电话</name>
      <key>phone</key>
      <type>text</type>
      <displayinlist>1</displayinlist>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>申请人QQ</name>
      <key>qq</key>
      <type>text</type>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>公司名称</name>
      <key>company_name</key>
      <type>text</type>
      <displayinlist>1</displayinlist>
      <search>1</search>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>公司所在地</name>
      <key>company_city</key>
      <type>text</type>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>公司地址</name>
      <key>company_address</key>
      <type>longtext</type>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>公司电话</name>
      <key>company_phone</key>
      <type>text</type>
      <search>1</search>
      <displayinlist>1</displayinlist>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>公司网址</name>
      <key>company_website</key>
      <type>text</type>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>是否了解</name>
      <key>knew</key>
      <type>check</type>
      <displayinlist>1</displayinlist>
      <yvalue>是</yvalue>
      <nvalue>否</nvalue>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>申请人留言</name>
      <key>message</key>
      <type>longtext</type>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>申请人疑问</name>
      <key>question</key>
      <type>longtext</type>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>处理状态</name>
      <key>status</key>
      <type>select</type>
      <displayinlist>1</displayinlist>
      <options>
        <option>
          <name>未处理</name>
          <value>P</value>
        </option>
        <option>
          <name>已完成</name>
          <value>A</value>
        </option>
        <option>
          <name>不通过</name>
          <value>I</value>
        </option>
      </options>
      <search>1</search>
      <notnull>1</notnull>
    </field>
    <field>
      <name>申请日期</name>
      <key>applied_date</key>
      <type>datetime</type>
      <displayinlist>1</displayinlist>
      <search>1</search>
      <notnull>1</notnull>
    </field>
    <field>
      <name>申请备注</name>
      <key>remarks</key>
      <type>longtext</type>
      <notnull>1</notnull>
    </field>
  </fields>
  <javascripts>
    <javascript>
      <filename>requisition.js</filename>
      <detail>1</detail>
      <search>1</search>
    </javascript>
    <javascript>
    </javascript>
  </javascripts>
</root>
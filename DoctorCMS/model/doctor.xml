        <option>
          <name>男性</name>
          <name_zh-cn>男性</name_zh-cn>
          <name_zh-hk>男性</name_zh-hk>
          <name_en-us>Male</name_en-us>
          <value>M</value>
        </option>
        <option>
          <name>女性</name>
          <name_zh-cn>女性</name_zh-cn>
          <name_zh-hk>女性</name_zh-hk>
          <name_en-us>Female</name_en-us>
          <name_zh-cn>状态</name_zh-cn>
          <name_zh-hk>状态</name_zh-hk>
          <name_en-us>Status</name_en-us>
          <value>F</value>
